{
  "name": "monitor",
  "type": "module",
  "version": "3.3.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "remix vite:build",
    "start": "npm run migrate && npm run start:server",
    "start:server": "cross-env NODE_ENV=production tsx ./server.ts",
    "migrate": "prisma migrate deploy",
    "dev": "cross-env NODE_ENV=development npx tsx ./server.ts",
    "format": "prettier --write .",
    "docker:build": "docker image build . -t bendiksolheim/monitor:latest -t bendiksolheim/monitor:3.3.0",
    "docker:push": "docker push bendiksolheim/monitor:latest && docker push bendiksolheim/monitor:3.3.0",
    "create-config": "mkdir -p config && cp config.example.json config/config.json",
    "generate-test-data": "cross-env NODE_ENV=development npx tsx test/generate-test-data.ts",
    "check-updates": "npx npm-check-updates",
    "update-all": "npx npm-check-updates -u"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@breejs/later": "^4.2.0",
    "@mantine/core": "^7.9.1",
    "@mantine/hooks": "^7.9.1",
    "@prisma/client": "^5.13.0",
    "@remix-run/css-bundle": "^2.9.1",
    "@remix-run/express": "^2.9.2",
    "@remix-run/node": "^2.9.1",
    "@remix-run/react": "^2.9.1",
    "@tabler/icons-react": "^3.3.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "node-cron": "^3.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remix-typedjson": "^0.4.1",
    "vite": "^5.2.11",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.9.1",
    "@types/breejs__later": "^4.1.5",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.12.11",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
