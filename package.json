{
  "name": "monitor",
  "type": "module",
  "version": "3.3.0",
  "packageManager": "pnpm@10.26.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "prisma generate",
    "build": "next build",
    "start": "pnpm run migrate && pnpm run start:server",
    "start:server": "cross-env NODE_ENV=production tsx ./server-nextjs.ts",
    "migrate": "prisma migrate deploy",
    "dev": "cross-env NODE_ENV=development tsx ./server-nextjs.ts",
    "format": "prettier --write .",
    "docker:build": "next build && docker image build . -t bendiksolheim/monitor:latest -t bendiksolheim/monitor:3.3.0",
    "docker:push": "docker push bendiksolheim/monitor:latest && docker push bendiksolheim/monitor:3.3.0",
    "create-config": "mkdir -p config && cp config.example.json config/config.json",
    "generate-test-data": "cross-env NODE_ENV=development npx tsx test/generate-test-data.ts",
    "check-updates": "npx npm-check-updates",
    "update-all": "npx npm-check-updates -u",
    "test": "vitest run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@breejs/ts-worker": "^2.0.0",
    "@mantine/core": "^8.3.10",
    "@mantine/hooks": "^8.3.10",
    "@prisma/client": "^5.22.0",
    "@tabler/icons-react": "^3.36.0",
    "bree": "^9.2.6",
    "cross-env": "^10.1.0",
    "next": "^16.0.10",
    "picocolors": "^1.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zod": "^3.25.76",
    "zod-validation-error": "^3.5.4"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prisma": "^5.22.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
